{
  "name": "main",
  "displayName": "MoodGenius",
  "expo": {
    "name": "MoodGenius",
    "slug": "mood-genius",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#6366f1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.myeasysoftware.moodgenius",
      "supportsTablet": true,
      "jsEngine": "hermes",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#6366f1"
      },
      "jsEngine": "hermes",
      "googleServicesFile": "./google-services.json",
      "package": "com.myeasysoftware.moodgenius"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-dev-client",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "sdkVersion": "53.0.0"
  }
}
