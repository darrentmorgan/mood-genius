    "@testing-library/react-native": "^13.2.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
